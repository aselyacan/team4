pipeline{

agent any
  
  stages{
  
    stage("build"){
      steps{
      echo 'building the application...'
        echo 'Application build ...'
      }
    }
    
    stage("test"){
      steps{
      echo 'testing the application...'
        echo 'Test build after changes ...'
      }
    }
  
    stage("deploy"){
      steps{
      echo 'deploying the application...'
      }
    }
    
    
    
  }
  
}
